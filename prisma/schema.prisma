generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Waitlist {
  id        String   @id @default(cuid())
  name      String
  email     String   @unique
  phone     String
  consent   Boolean
  createdAt DateTime @default(now())
}

